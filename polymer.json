{
  "entrypoint": "index.html",
  "shell": "src/money-track.html",
  "builds": [{
    "preset": "es6-unbundled"
  }],
  "fragments": [
    "src/login-page.html",
    "src/main-page.html",
    "src/dashboard-page.html",
    "src/income-page.html",
    "src/expense-page.html",
    "src/categories-page.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "php/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "bower_components/moment/min/moment.min.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  }
}
